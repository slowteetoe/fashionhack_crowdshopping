{
  "manifest_version": 2,

  "name": "CrowdShopping",
  "description": "Buy stuff people tag",
  "version": "1.0",

  "permissions": [
    "http://fashionhack.com:4000/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
   "content_scripts": [
    {
      "matches": ["http://fashionhack.com/*"],
      "js": ["myscript.js"]
    }
  ]
}